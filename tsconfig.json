{
  "compilerOptions": {
    "target": "ES2015", // or a later version
    "module": "commonjs", // or "esnext" if you're using ES modules
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
  }
}